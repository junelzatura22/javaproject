		<![CDATA[SELECT  e_yearLevel, e_semester,e_schoolyear,rname,
subject as subjects,time as timestart,
date as schedule,timeend,
sub_unit,sub_description,c_name,c_description, c_department,
concat(stud_fname,' ',SUBSTRING(stud_mname,1,1),'. ',stud_lname) as studFullname, stud_status,
e_room,rname, rinstalledlocation
FROM enrollment e,lecture l,lecturer lec,course c,student stud, subjects subs,room ro
where e.e_lecture = l.lectureid
and subs.sub_code = l.subject
and lec.lec_id = l.lecturer
and c.courseid = e.e_course
and ro.rid = e.e_room
and stud.studentid = e.e_student and e.e_schoolyear = $P{inclusiveYear} and studentid = $P{studentNum} and e_semester = $P{semester}]]>
	</queryString>
