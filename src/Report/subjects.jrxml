	<parameter name="semester" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT subject,sub_description,time as startTime,timeend as timeEnd,date as schedule,
count(stud.studentid) as totalStudent

FROM enrollment e,lecture l,lecturer lec,course c,student stud, subjects subs,room ro

where e.e_lecture = l.lectureid
and subs.sub_code = l.subject
and lec.lec_id = l.lecturer
and c.courseid = e.e_course
and ro.rid = e.e_room

and stud.studentid = e.e_student and e.e_schoolyear = $P{schoolYear} and e.e_semester  = $P{semester}
group by l.lectureid order by c_name asc]]>
	</queryString>
